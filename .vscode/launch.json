    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "env": {
                "DBHOST": "192.168.1.41",
                "google.gzperm.client_id":"test.apps.googleusercontent.com",
                "google.gzperm.client_secret":"test"
            },
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/server.ts",
            "preLaunchTask": "tsc: build - tsconfig.json",
            "smartStep": true,
            "outFiles": [
                "${workspaceFolder}/build/**/*.js"
            ]
        }
    ]
}